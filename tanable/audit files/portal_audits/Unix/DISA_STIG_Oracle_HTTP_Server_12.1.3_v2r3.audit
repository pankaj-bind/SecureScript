      cmd         : "find @DOMAIN_HOME@/config/fmwconfig/components/OHS/@COMPONENT_NAME@ -iname \"*.conf\" ! -iname \"admin.conf\" -print | xargs awk '/^[\\s]*<\\/VirtualHost>/ { if(text==NULL){text=\"SSLEngine not found\"}; print FILENAME\":\"l\": \"text; } /^[\\s]*SSLEngine.*$/ { text=$0; } /^[\\s]*<VirtualHost/ { l=FNR;text=NULL }'"
