      see_also    : "https://dl.dod.cyber.mil/wp-content/uploads/stigs/zip/U_Oracle_HTTP_Server_12-1-3_V2R3_STIG.zip"
      cmd         : "awk 'BEGIN { d = 0; f = \"\"; failures = 0; } /<Directory/ { d = 1; t = $0; } d == 1 && /^[\\s]*[Oo]ptions/ { f = $0; } d == 1 && /<\\/Directory>/ { if (f ~ /^[\\s]*[Oo]ptions[\\s]*[Nn]one/) { print t,f,\"Pass\"} else { print t,f,\"Fail\"; failures++; }; d = 0; t = \"\"; f = \"\"; } END { print \"Number of failures: \" failures }' @DOMAIN_HOME@/config/fmwconfig/components/OHS/@COMPONENT_NAME@/*.conf"
