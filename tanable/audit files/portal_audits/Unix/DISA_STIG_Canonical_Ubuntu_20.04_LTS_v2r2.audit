      see_also    : "https://dl.dod.cyber.mil/wp-content/uploads/stigs/zip/U_CAN_Ubuntu_20-04_LTS_V2R2_STIG.zip"
      cmd         : "/bin/find /var/log -perm /137 ! -name '*[bw]tmp' ! -name '*lastlog' -type f -exec /bin/stat -c \"%n %a\" {} \\;  | /bin/awk '{print} END {if(NR==0) print \"none found\" }'"
