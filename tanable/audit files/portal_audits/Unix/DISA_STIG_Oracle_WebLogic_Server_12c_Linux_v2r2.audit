      xsl_stmt    : "<xsl:for-each select=\"//*[local-name() = 'server']\">"
      xsl_stmt    : "Listen Port Enabled for server '<xsl:value-of select=\"./*[local-name() = 'name']\"/>': <xsl:value-of select=\"./*[local-name() = 'listen-port-enabled']\" />"
