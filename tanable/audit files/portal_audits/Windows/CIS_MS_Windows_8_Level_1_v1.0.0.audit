<check_type:"Windows" version:"2">
<group_policy:"MS Windows 8">

<if>
  <condition type:"AND">
    <custom_item>
      type        : REGISTRY_SETTING
      description : "Windows 8 is installed"
      value_type  : POLICY_TEXT
      value_data  : "^[Ww][Ii][Nn][Dd][Oo][Ww][Ss] @PLATFORM_VERSION@.+$"
      reg_key     : "HKLM\SOFTWARE\Microsoft\Windows NT\CurrentVersion"
      reg_item    : "ProductName"
      check_type  : CHECK_REGEX
    </custom_item>
  </condition>

  <then>
    <report type:"PASSED">
      description : "CIS_MS_Windows_8_Level_1_v1.0.0.audit Level 1"
    </report>

## 1 Computer Configuration
## 1.1 Security Settings
## 1.1.1 Account Policies

    <custom_item>
      type           : LOCKOUT_POLICY
      description    : "1.1.1.1 Set 'Account lockout threshold' to '5 invalid logon attempt(s)'"
      info           : "This policy setting determines the number of failed logon attempts before a lock occurs."
      solution       : "Make sure 'Account lockout threshold' is set to 5 invalid attempts."
      reference      : "800-171|3.1.8,800-53|AC-7a.,800-53r5|AC-7a.,CN-L3|8.1.4.1(b),CSCv6|16.7,CSF2.0|PR.AA-03,GDPR|32.1.b,HIPAA|164.306(a)(1),ITSG-33|AC-7a.,LEVEL|1S,NESA|T5.5.1,NIAv2|AM24,PCI-DSSv3.2.1|8.1.6,PCI-DSSv4.0|8.3.4,TBA-FIISB|45.1.2,TBA-FIISB|45.2.1,TBA-FIISB|45.2.2"
      see_also       : "https://workbench.cisecurity.org/files/17"
      value_type     : POLICY_DWORD
      value_data     : [1..5]
      lockout_policy : LOCKOUT_THRESHOLD
    </custom_item>

    <custom_item>
      type           : LOCKOUT_POLICY
      description    : "1.1.1.2 Set 'Account lockout duration' to '15 or more minute(s)'"
      info           : "This policy setting determines the length of time that must pass before a locked account is unlocked and a"
      info           : "user can try to log on again."
      solution       : "Make sure 'Account lockout duration' is set to a minimum of 15 minutes."
      reference      : "800-171|3.1.8,800-53|AC-7a.,800-53r5|AC-7a.,CN-L3|8.1.4.1(b),CSCv6|16.7,CSF2.0|PR.AA-03,GDPR|32.1.b,HIPAA|164.306(a)(1),ITSG-33|AC-7a.,LEVEL|1S,NESA|T5.5.1,NIAv2|AM24,PCI-DSSv3.2.1|8.1.6,PCI-DSSv4.0|8.3.4,TBA-FIISB|45.1.2,TBA-FIISB|45.2.1,TBA-FIISB|45.2.2"
      see_also       : "https://workbench.cisecurity.org/files/17"
      value_type     : TIME_MINUTE
      value_data     : [15..MAX]
      lockout_policy : LOCKOUT_DURATION
    </custom_item>

    <custom_item>
      type           : LOCKOUT_POLICY
      description    : "1.1.1.3 Set 'Reset account lockout counter after' to '15 minute(s)'"
      info           : "This policy setting determines the length of time before the Account lockout threshold resets to zero."
      solution       : "Make sure 'Reset account lockout counter after' is set to a minimum of 15 minutes."
      reference      : "800-171|3.1.8,800-53|AC-7a.,800-53r5|AC-7a.,CN-L3|8.1.4.1(b),CSCv6|16.7,CSF2.0|PR.AA-03,GDPR|32.1.b,HIPAA|164.306(a)(1),ITSG-33|AC-7a.,LEVEL|1S,NESA|T5.5.1,NIAv2|AM24,PCI-DSSv3.2.1|8.1.6,PCI-DSSv4.0|8.3.4,TBA-FIISB|45.1.2,TBA-FIISB|45.2.1,TBA-FIISB|45.2.2"
      see_also       : "https://workbench.cisecurity.org/files/17"
      value_type     : TIME_MINUTE
      value_data     : [15..MAX]
      lockout_policy : LOCKOUT_RESET
    </custom_item>

    <custom_item>
      type            : PASSWORD_POLICY
      description     : "1.1.1.4 Set 'Minimum password length' to '14 or more character(s)'"
      info            : "This policy setting determines the least number of characters that make up a password for a user account."
      solution        : "Make sure 'Minimum password length' is set to a minimum of 14 characters."
      reference       : "800-171|3.5.7,800-53|IA-5(1)(a),800-53r5|IA-5(1)(a),CN-L3|7.1.2.7(e),CN-L3|7.1.3.1(b),CSCv6|5.7,CSCv6|16.2,CSCv6|16.5,CSF|PR.AC-1,CSF2.0|PR.AA-01,CSF2.0|PR.AA-03,GDPR|32.1.b,HIPAA|164.306(a)(1),HIPAA|164.312(a)(2)(i),HIPAA|164.312(d),ISO/IEC-27001|A.9.4.3,ITSG-33|IA-5(1)(a),LEVEL|1S,NESA|T5.2.3,NIAv2|AM19a,NIAv2|AM19b,NIAv2|AM19c,NIAv2|AM19d,NIAv2|AM22a,QCSC-v1|5.2.2,QCSC-v1|13.2,SWIFT-CSCv1|4.1,TBA-FIISB|26.2.1,TBA-FIISB|26.2.4"
      see_also        : "https://workbench.cisecurity.org/files/17"
      value_type      : POLICY_DWORD
      value_data      : @MINIMUM_PASSWORD_LENGTH@
      password_policy : MINIMUM_PASSWORD_LENGTH
    </custom_item>

    <custom_item>
      type            : PASSWORD_POLICY
      description     : "1.1.1.5 Set 'Enforce password history' to '24 or more password(s)'"
      info            : "This policy setting determines the number of renewed, unique passwords that have to be associated with a user"
      info            : "account before you can reuse an old password."
      solution        : "Make sure 'Enforce password history' is set to a minimum of 24 passwords."
      reference       : "800-171|3.5.8,800-53|IA-5(1)(e),800-53r5|IA-5(1)(e),CSCv6|16.5,CSF|PR.AC-1,CSF2.0|PR.AA-01,CSF2.0|PR.AA-03,GDPR|32.1.b,HIPAA|164.306(a)(1),HIPAA|164.312(a)(2)(i),HIPAA|164.312(d),ISO/IEC-27001|A.9.4.3,ITSG-33|IA-5(1)(e),LEVEL|1S,NESA|T5.2.3,NIAv2|AM22c,QCSC-v1|5.2.2,QCSC-v1|13.2,SWIFT-CSCv1|4.1,TBA-FIISB|26.2.3"
      see_also        : "https://workbench.cisecurity.org/files/17"
      value_type      : POLICY_DWORD
      value_data      : @PASSWORD_HISTORY@
      password_policy : ENFORCE_PASSWORD_HISTORY
    </custom_item>

    <custom_item>
      type            : PASSWORD_POLICY
      description     : "1.1.1.6 Set 'Password must meet complexity requirements' to 'Enabled'"
      info            : "This policy setting checks all new passwords to ensure that they meet basic requirements for strong passwords."
      solution        : "Make sure 'Password must meet complexity requirements' is set to a minimum of 24 passwords."
      reference       : "800-171|3.5.7,800-53|IA-5(1)(a),800-53r5|IA-5(1)(a),CN-L3|7.1.2.7(e),CN-L3|7.1.3.1(b),CSF|PR.AC-1,CSF2.0|PR.AA-01,CSF2.0|PR.AA-03,GDPR|32.1.b,HIPAA|164.306(a)(1),HIPAA|164.312(a)(2)(i),HIPAA|164.312(d),ISO/IEC-27001|A.9.4.3,ITSG-33|IA-5(1)(a),LEVEL|1S,NESA|T5.2.3,NIAv2|AM19a,NIAv2|AM19b,NIAv2|AM19c,NIAv2|AM19d,NIAv2|AM22a,QCSC-v1|5.2.2,QCSC-v1|13.2,SWIFT-CSCv1|4.1,TBA-FIISB|26.2.1,TBA-FIISB|26.2.4"
      see_also        : "https://workbench.cisecurity.org/files/17"
      value_type      : POLICY_SET
      value_data      : "Enabled"
      password_policy : COMPLEXITY_REQUIREMENTS
    </custom_item>

    <custom_item>
      type            : PASSWORD_POLICY
      description     : "1.1.1.7 Set 'Store passwords using reversible encryption' to 'Disabled'"
      info            : "This policy setting determines whether the operating system stores passwords in a way that uses reversible"
      info            : "encryption, which provides support for application protocols that require knowledge of the user's password"
      info            : "for authentication purposes."
      solution        : "Make sure 'Store passwords using reversible encryption' is disabled."
      reference       : "800-171|3.5.10,800-53|IA-5(1)(c),800-53r5|IA-5(1)(c),CSCv6|16.5,CSCv6|16.13,CSCv6|16.14,CSF|PR.AC-1,CSF2.0|PR.AA-01,CSF2.0|PR.AA-03,GDPR|32.1.b,HIPAA|164.306(a)(1),HIPAA|164.312(a)(2)(i),HIPAA|164.312(d),ITSG-33|IA-5(1)(c),LEVEL|1S,NESA|T5.2.3,NIAv2|CY6,QCSC-v1|5.2.2,QCSC-v1|13.2,SWIFT-CSCv1|4.1,TBA-FIISB|26.1"
      see_also        : "https://workbench.cisecurity.org/files/17"
      value_type      : POLICY_SET
      value_data      : "Disabled"
      password_policy : REVERSIBLE_ENCRYPTION
    </custom_item>

    <custom_item>
      type            : PASSWORD_POLICY
      description     : "1.1.1.8 Set 'Minimum password age' to '1 or more day(s)'"
      info            : "This policy setting determines the number of days that you must use a password before you can change it."
      solution        : "Make sure minimum password age is set to a minimum of 1 day."
      reference       : "800-171|3.5.2,800-53|IA-5(1)(d),800-53r5|IA-5(1)(d),CN-L3|7.1.2.7(e),CN-L3|7.1.3.1(b),CSCv6|16.5,CSF|PR.AC-1,CSF2.0|PR.AA-01,CSF2.0|PR.AA-03,GDPR|32.1.b,HIPAA|164.306(a)(1),HIPAA|164.312(a)(2)(i),HIPAA|164.312(d),ISO/IEC-27001|A.9.4.3,ITSG-33|IA-5(1)(d),LEVEL|1S,NESA|T5.2.3,NIAv2|AM20,NIAv2|AM21,QCSC-v1|5.2.2,QCSC-v1|13.2,SWIFT-CSCv1|4.1,TBA-FIISB|26.2.2"
      see_also        : "https://workbench.cisecurity.org/files/17"
      value_type      : TIME_DAY
      value_data      : @MINIMUM_PASSWORD_AGE@
      password_policy : MINIMUM_PASSWORD_AGE
    </custom_item>

    <custom_item>
      type            : PASSWORD_POLICY
      description     : "1.1.1.9 Set 'Maximum password age' to '60 or fewer days'"
      info            : "This policy setting defines how long a user can use their password before it expires."
      solution        : "Make sure 'maximum password age' is set to a minimum of 60 days."
      reference       : "800-171|3.5.2,800-53|IA-5(1)(d),800-53r5|IA-5(1)(d),CN-L3|7.1.2.7(e),CN-L3|7.1.3.1(b),CSCv6|16.5,CSF|PR.AC-1,CSF2.0|PR.AA-01,CSF2.0|PR.AA-03,GDPR|32.1.b,HIPAA|164.306(a)(1),HIPAA|164.312(a)(2)(i),HIPAA|164.312(d),ISO/IEC-27001|A.9.4.3,ITSG-33|IA-5(1)(d),LEVEL|1S,NESA|T5.2.3,NIAv2|AM20,NIAv2|AM21,QCSC-v1|5.2.2,QCSC-v1|13.2,SWIFT-CSCv1|4.1,TBA-FIISB|26.2.2"
      see_also        : "https://workbench.cisecurity.org/files/17"
      value_type      : TIME_DAY
      value_data      : @MAXIMUM_PASSWORD_AGE@
      password_policy : MAXIMUM_PASSWORD_AGE
    </custom_item>

