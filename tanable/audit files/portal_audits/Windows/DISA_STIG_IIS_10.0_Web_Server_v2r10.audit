<check_type:"Windows" version:"2">
<group_policy:"DISA Microsoft IIS 10.0 Server">

<file_acl:"inetpub">
  <user:"Administrators">
    acl_inheritance : "not inherited"
    acl_apply       : "This folder, subfolders and files"
    acl_allow       : "change permissions" | "create files / write data" | "create folders / append data" | "delete" | "delete subfolder and files" | "full control" | "list folder / read data" | "read attributes" | "read extended attributes" | "read permissions" | "take ownership" | "traverse folder / execute file" | "write attributes" | "write extended attributes"
  </user>

  <user:"SYSTEM">
    acl_inheritance : "not inherited"
    acl_apply       : "This folder, subfolders and files"
    acl_allow       : "change permissions" | "create files / write data" | "create folders / append data" | "delete" | "delete subfolder and files" | "full control" | "list folder / read data" | "read attributes" | "read extended attributes" | "read permissions" | "take ownership" | "traverse folder / execute file" | "write attributes" | "write extended attributes"
  </user>

  <user:"TrustedInstaller">
    acl_inheritance : "not inherited"
    acl_apply       : "This folder, subfolders and files"
    acl_allow       : "change permissions" | "create files / write data" | "create folders / append data" | "delete" | "delete subfolder and files" | "full control" | "list folder / read data" | "read attributes" | "read extended attributes" | "read permissions" | "take ownership" | "traverse folder / execute file" | "write attributes" | "write extended attributes"
  </user>

  <user:"Users">
    acl_inheritance : "not inherited"
    acl_apply       : "This folder, subfolders and files"
    acl_allow       : "list folder / read data" | "read attributes" | "read extended attributes" | "read permissions" | "traverse folder / execute file"
  </user>

  <user:"Creator Owner">
    acl_inheritance : "not inherited"
    acl_apply       : "subfolders and files only"
    acl_allow       : "change permissions" | "create files / write data" | "create folders / append data" | "delete" | "delete subfolder and files" | "full control" | "list folder / read data" | "read attributes" | "read extended attributes" | "read permissions" | "take ownership" | "traverse folder / execute file" | "write attributes" | "write extended attributes"
  </user>

  <user:"ALL APPLICATION PACKAGES">
    acl_inheritance : "not inherited"
    acl_apply       : "This folder, subfolders and files"
    acl_allow       : "list folder / read data" | "read attributes" | "read extended attributes" | "read permissions" | "traverse folder / execute file"
  </user>
</file_acl>

<file_acl:"inetmgr">
  <user:"Administrators">
    acl_inheritance : "not inherited"
    acl_apply       : "This object only"
    acl_allow       : "list folder / read data" | "read attributes" | "read extended attributes" | "read permissions" | "traverse folder / execute file"
  </user>

  <user:"SYSTEM">
    acl_inheritance : "not inherited"
    acl_apply       : "This object only"
    acl_allow       : "list folder / read data" | "read attributes" | "read extended attributes" | "read permissions" | "traverse folder / execute file"
  </user>

  <user:"TrustedInstaller">
    acl_inheritance : "not inherited"
    acl_apply       : "This object only"
    acl_allow       : "change permissions" | "create files / write data" | "create folders / append data" | "delete" | "delete subfolder and files" | "full control" | "list folder / read data" | "read attributes" | "read extended attributes" | "read permissions" | "take ownership" | "traverse folder / execute file" | "write attributes" | "write extended attributes"
  </user>

  <user:"Web Managers">
    acl_inheritance : "not inherited"
    acl_apply       : "This object only"
    acl_allow       : "change permissions" | "create files / write data" | "create folders / append data" | "delete" | "delete subfolder and files" | "full control" | "list folder / read data" | "read attributes" | "read extended attributes" | "read permissions" | "take ownership" | "traverse folder / execute file" | "write attributes" | "write extended attributes"
  </user>

  <user:"Web Manager designees">
    acl_inheritance : "not inherited"
    acl_apply       : "This object only"
    acl_allow       : "change permissions" | "create files / write data" | "create folders / append data" | "delete" | "delete subfolder and files" | "full control" | "list folder / read data" | "read attributes" | "read extended attributes" | "read permissions" | "take ownership" | "traverse folder / execute file" | "write attributes" | "write extended attributes"
  </user>

  <user:"ALL APPLICATION PACKAGES">
    acl_inheritance : "not inherited"
    acl_apply       : "This folder, subfolders and files"
    acl_allow       : "list folder / read data" | "read attributes" | "read extended attributes" | "read permissions" | "traverse folder / execute file"
  </user>

  <user:"Users">
    acl_inheritance : "not inherited"
    acl_apply       : "This folder, subfolders and files"
    acl_allow       : "list folder / read data" | "read attributes" | "read extended attributes" | "read permissions" | "traverse folder / execute file"
  </user>
</file_acl>

<if>
  <condition type:"AND">
    <custom_item>
      type        : REGISTRY_SETTING
      description : "Verify IIS is installed."
      value_type  : POLICY_TEXT
      value_data  : "(internet|iis)"
      reg_key     : "HKLM\SYSTEM\CurrentControlSet\Services\W3SVC"
      reg_item    : "ImagePath"
      check_type  : CHECK_REGEX
    </custom_item>

    <custom_item>
      type        : REGISTRY_SETTING
      description : "Verify IIS 10 installed."
      value_type  : POLICY_TEXT
      value_data  : "^Version @PRODUCT_VERSION@[\s]*$"
      reg_key     : "HKLM\Software\Microsoft\Inetstp"
      reg_item    : "VersionString"
      check_type  : CHECK_REGEX
    </custom_item>

    <custom_item>
      type        : REGISTRY_SETTING
      description : "Windows Server 2016/2019/2022 and Windows 10"
      value_type  : POLICY_TEXT
      value_data  : "^[a-zA-Z0-9\(\)\s]*@PLATFORM_VERSION@[a-zA-Z0-9\(\)\s]*$"
      reg_key     : "HKLM\Software\Microsoft\Windows Nt\Currentversion"
      reg_item    : "ProductName"
      check_type  : CHECK_REGEX
    </custom_item>
  </condition>

  <then>
    <report type:"PASSED">
      description : "DISA_STIG_IIS_10.0_Web_Server_v2r10.audit from DISA Microsoft IIS 10.0 Server v2r10 STIG"
      see_also    : "https://dl.dod.cyber.mil/wp-content/uploads/stigs/zip/U_MS_IIS_10-0_Y23M10_STIG.zip"
    </report>

    <if>
      <condition auto:"FAILED" type:"AND">
        <custom_item>
          type        : AUDIT_IIS_APPCMD
          description : "'W3C' format is applied"
          value_type  : POLICY_TEXT
          value_data  : "W3C"
          appcmd_args : "list config /section:system.applicationHost/sites /text:siteDefaults.logfile.logFormat"
        </custom_item>

        <custom_item>
          type        : AUDIT_IIS_APPCMD
          description : "Verify field 'Date' is selected"
          value_type  : POLICY_TEXT
          value_data  : "date"
          appcmd_args : "list config /section:system.applicationHost/sites /text:siteDefaults.logfile.logExtFileFlags"
          check_type  : CHECK_REGEX
        </custom_item>

        <custom_item>
          type        : AUDIT_IIS_APPCMD
          description : "Verify field 'Time' is selected"
          value_type  : POLICY_TEXT
          value_data  : "time"
          appcmd_args : "list config /section:system.applicationHost/sites /text:siteDefaults.logfile.logExtFileFlags"
          check_type  : CHECK_REGEX
