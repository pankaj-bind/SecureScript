      see_also         : "https://workbench.cisecurity.org/files/1780"
      api_request_type : "op"
      request          : "<show><config><@CONFIG_TYPE@></@CONFIG_TYPE@></config></show>"
      xsl_stmt         : "<xsl:template match=\"/\">"
