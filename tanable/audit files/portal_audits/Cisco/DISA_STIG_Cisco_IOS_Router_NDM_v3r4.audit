R1(config-line)#exec-timeout 5 0
R1(config-line)#exit
R1(config)#line con 0
R1(config-line)#exec-timeout 5 0
R1(config-line)#exit
R2(config)#ip http timeout-policy idle 300 life nnnn requests nn"
