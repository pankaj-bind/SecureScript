  see_also    : "https://dl.dod.cyber.mil/wp-content/uploads/stigs/zip/U_Arista_MLS_DCS-7000_Series_Y24M07_STIG.zip"
  regex       : "ntp server @NTP_SERVER_1@"
  expect      : "^[\s]*ntp server @NTP_SERVER_1@ (prefer)?"
