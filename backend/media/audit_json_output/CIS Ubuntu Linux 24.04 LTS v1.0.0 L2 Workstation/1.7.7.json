{
  "check_type": "CONDITIONAL",
  "condition": {
    "type": "AND",
    "rules": [
      {
        "type": "CMD_EXEC",
        "description": "GDM is installed",
        "cmd": "/bin/dpkg -s gdm3 2>&1 | /bin/grep -E '(^Status:|not installed)'",
        "expect": "^Status: install ok"
      }
    ]
  }
}